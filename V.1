import math
a = 0
b = 0

#ask for margin, lines with bounds, circles, scale,
def dist(t,a,b):
    x = t[0]
    y = t[1]
    c = (x-a)**2
    d = (y-b)**2
    fin = math.sqrt(c+d)
    return fin

test = (1,20)
margin = 3
radius = 3
print(dist(test,a,b))
output = []
points = [(1,2), (3,4), (4,5), (1,20), (2,5), (10,100)]

for point in points:
    if dist(point,a,b) <= (radius + margin):
        if dist(point,a,b) >= (radius - margin):
            output.append(point)

print(output)
